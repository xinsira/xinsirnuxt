    "kcors": "^2.2.2",
    "koa": "^2.6.2",
    "nuxt": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "nodemon": "^1.19.4",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  }
